{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "declaration": true,
        "sourceMap": true,
        "outDir": "js"
    }
}